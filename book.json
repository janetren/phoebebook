{
    "plugins": [
        "github",
        "theme-api",
        "ace",
        "emphasize",
        "include-codeblock",
        "katex",
        "-mermaid"
    ],
    "pluginsConfig": {
        "fontSettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "java",
                    "name": "Java"
                }
            ]
        },
        "github": {
            "url": "https://github.com/lynnhawk/phoebebook.git"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy www.pthink.com.cn 2016",
            "modify_label": "modified: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    },
    "author": "phy",
    "pdf": {
        "pageNumbers": false,
        "margin": {
            "top": 36,
            "bottom": 36
        },
        "toc": true
    },
    "title": "phoebe book",
    "description": "phoebe book"
}